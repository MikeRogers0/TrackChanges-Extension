{
  "name": "__MSG_name__",
  "short_name": "__MSG_short_name__",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "devtools_page": "lib/devtools.html",
  "background": {
    "scripts": [
      "lib/background/scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lib/content_scripts/on_inject_callback.js"]
    }
  ],
  "default_locale": "en",
  "permissions": [
    "tabs", "pageCapture", "activeTab"
  ],
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "minimum_chrome_version": "55",
  "offline_enabled": true,
  "author": "Mike Rogers",
  "homepage_url": "https://mikerogers.io/changes-diff"
}
